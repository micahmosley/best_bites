<div class="row">
<div class="column">
<ol>
<% @top_five.each do |dessert|%>
    <li class="bite">
    <image class="top" src="<%=(url_for(dessert.photo))%>">
    <%=dessert.name%><br>
    <%="#{dessert.number_of_favorites} favorites!!!"%><br>
    
    <%= link_to "See #{dessert.name}", dessert_path(dessert)%><br><br><br>
    </li>
<%end%>
</ol>
</div>
<div class="column">
<ol start="6">
<% @second_five.each do |dessert|%>
    <li class="bite">
    <image class="top" src="<%=(url_for(dessert.photo))%>">
    <%=dessert.name%><br>
    <%="#{dessert.number_of_favorites} favorites!!!"%><br>
    <%= link_to "See #{dessert.name}", dessert_path(dessert)%><br><br><br>
    </li>
<%end%>
</ol>
</div>
</div>